<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.fei.dao.Rong" table = "userrongyun">
		<id name = "id" column = "id" type = "int">
			<generator class="increment" />
		</id>
		
		<property name="userName" column = "userName" type = "string" not-null = "true" unique = "true" />
		<property name="rongToken" column = "rongToken" type = "string" />
		<property name="name" column = "name" type = "string" />
		<property name="portraitUri" column = "portraitUri" type = "string" />
	</class>
</hibernate-mapping>