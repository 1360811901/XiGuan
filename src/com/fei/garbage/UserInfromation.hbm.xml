<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.fei.dao.UserInfromation" table = "userInfromation">
		<id name = "id" column = "id" type = "int">
			<generator class = "increment" />
		</id>
		
		<property name="userName" column = "userName" type = "string" not-null = "true" unique = "true" />
		<property name="name" column = "name" type = "string" />
		<property name="sex" column = "sex" type = "string" />
		<property name="age" column = "age" type = "java.lang.Integer" />
		<property name="career" column = "career" type = "string" />
		<property name="phoneNumber" column = "phoneNumber" type = "string" />
		<property name="email" column = "email" type = "string" />
		<property name="address" column = "address" type = "string" />
		<property name="portraitUri" column = "portraitUri" type = "string" />
	</class>
</hibernate-mapping>