<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.fei.Entity.FocusOn" table = "focuson">
		<id name = "id" column = "id" type = "int">
			<generator class = "increment" />
		</id>
		<property name = "userName" column = "username" type = "string" />
		<property name = "focusOn_userName" column = "focuson_username" type = "string" />
		<property name = "fTime" column = "ftime" type = "string" />
		<property name = "focusOnNumber" column = "focusonnumber" type = "string" />
		<property name = "fansNumber" column = "fansnumber" type = "string" />
	</class>
</hibernate-mapping>