<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.fei.Entity.UserAlbum" table = "useralbum">
		<id name = "id" column = "id" type = "int">
			<generator class = "increment" />
		</id>
		<property name = "url" column = "url" type = "string" />
		<property name = "userInfoId" column = "userinfoid" type = "string" />
		<property name = "photetype" column = "photetype" type = "string" />
		<property name = "fileName" column = "filename" type = "string" />
		<property name = "size" column = "size" type = "string" />
		<property name = "sort" column = "sort" type = "string" />
		<property name = "isDel" column = "isdel" type = "string" />
	</class>
</hibernate-mapping>